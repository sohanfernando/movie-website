# Database Configuration
DB_URL=jdbc:mysql://localhost:3306/movieDB
DB_USERNAME=root
DB_PASSWORD=your_secure_password

# Server Configuration
SERVER_PORT=8080

# Security Configuration
JWT_SECRET=your_very_long_and_secure_jwt_secret_key_here
